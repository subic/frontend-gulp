{
  "name": "frontend-build-tools",
  "version": "0.6.0",
  "description": "A gulp toolkit for a simple webpage front end build with some defaults which can be extended and customized.",
  "main": "gulpfile.js",
  "scripts": {
    "createDirectories": "(md img || mkdir img) && (md css || mkdir css) && (md js || mkdir js) && (md root || mkdir root)",
    "createFiles": "echo $null >> css/main.css && echo $null >> js/main.js",
    "build": "npm run createDirectories && npm run createFiles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/subic/frontend-gulp"
  },
  "author": "Jaka Å ubic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subic/frontend-gulp/issues"
  },
  "homepage": "https://github.com/subic/frontend-gulp",
  "devDependencies": {
    "autoprefixer": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.11.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "webpack-stream": "^3.2.0",
    "lost": "^6.6.2",
    "postcss-cssnext": "^2.4.0",
    "precss": "^1.3.0",
    "run-sequence": "^1.0.2",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.10.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-svg-sprite": "^1.2.19",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.1.0"
  }
}
