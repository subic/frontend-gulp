{
  "name": "frontend-gulp",
  "version": "0.7.3",
  "description": "A gulp toolkit for a simple webpage front end build with some defaults which can be extended and customized.",
  "main": "gulpfile.js",
  "scripts": {
    "exampleDirectories": "(md img || mkdir img) && (md css || mkdir css) && (md js || mkdir js) && (md root || mkdir root) && (md html || mkdir html)",
    "createIndex": "echo \"<!doctype html><html><head><link rel=\"stylesheet\" href=\"style.css\"></head><body><script src=\"main.js\"></script></body></html>\" >> ./index.html",
    "exampleFiles": "echo $null >> css/main.css && echo $null >> js/main.js && npm run createIndex",
    "build": "npm run exampleDirectories && npm run exampleFiles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/subic/frontend-gulp"
  },
  "author": "Jaka Å ubic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subic/frontend-gulp/issues"
  },
  "homepage": "https://github.com/subic/frontend-gulp",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-preset-es2015": "^6.22.0",
    "browser-sync": "^2.18.7",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.10.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.3.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-uglify": "^2.0.1",
    "lost": "^8.0.0",
    "postcss-cssnext": "^2.9.0",
    "precss": "^1.4.0",
    "run-sequence": "^1.2.2",
    "webpack-stream": "^3.2.0"
  }
}
